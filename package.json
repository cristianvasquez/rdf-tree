{
  "name": "rdf-tree",
  "type": "module",
  "version": "0.2.1",
  "description": "Vue.js component library for visualizing RDF datasets",
  "author": "",
  "license": "MIT",
  "main": "dist/rdf-tree.js",
  "module": "dist/rdf-tree.es.js",
  "exports": {
    ".": {
      "import": "./dist/rdf-tree.es.js",
      "require": "./dist/rdf-tree.js"
    },
    "./dist/rdf-tree.css": "./dist/rdf-tree.css"
  },
  "files": [
    "dist/**/*",
    "src/**/*"
  ],
  "scripts": {
    "dev:showcase": "vite --config vite.showcase.config.js",
    "build": "vite build --config vite.showcase.config.js",
    "build:lib": "vite build --mode lib",
    "preview:showcase": "vite preview --config vite.showcase.config.js",
    "snapshot": "mocha",
    "test": "mocha"
  },
  "dependencies": {
    "@rdfjs/parser-n3": "^2.1.0",
    "grapoi": "^1.1.3",
    "n3": "^1.26.0",
    "rdf-ext": "^2.5.2",
    "zod": "^3.25.76"
  },
  "peerDependencies": {
    "naive-ui": "^2.40.0",
    "vue": "^3.5.0"
  },
  "devDependencies": {
    "@vicons/ionicons5": "^0.13.0",
    "@vitejs/plugin-vue": "^5.2.4",
    "@vueuse/core": "^13.7.0",
    "expect": "^29.7.0",
    "expect-mocha-snapshot": "^2.1.5",
    "jsdom": "^26.1.0",
    "mocha": "^11.7.1",
    "naive-ui": "^2.42.0",
    "readable-stream": "^4.7.0",
    "vite": "^6.3.5",
    "vite-plugin-node-polyfills": "^0.23.0",
    "vue": "^3.5.20"
  }
}
